{% extends 'base.html.twig' %}

{% block title %}Staff{% endblock %}

{% block body %}
    <h1>Staff</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ staff.id }}</td>
            </tr>
            <tr>
                <th>First_name</th>
                <td>{{ staff.firstName }}</td>
            </tr>
            <tr>
                <th>Last_name</th>
                <td>{{ staff.lastName }}</td>
            </tr>
            <tr>
                <th>Username</th>
                <td>{{ staff.username }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ staff.password }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ staff.email }}</td>
            </tr>
            <tr>
                <th>Address1</th>
                <td>{{ staff.address1 }}</td>
            </tr>
            <tr>
                <th>Address2</th>
                <td>{{ staff.address2 }}</td>
            </tr>
            <tr>
                <th>Zipcode</th>
                <td>{{ staff.zipcode }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ staff.city }}</td>
            </tr>
            <tr>
                <th>Phone</th>
                <td>{{ staff.phone }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('staff_index') }}">back to list</a>

    <a href="{{ path('staff_edit', {'id': staff.id}) }}">edit</a>

    {{ include('staff/_delete_form.html.twig') }}
{% endblock %}
