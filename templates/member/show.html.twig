{% extends 'base.html.twig' %}

{% block title %}Member{% endblock %}

{% block body %}
    <h1>Member</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ member.id }}</td>
            </tr>
            <tr>
                <th>First_name</th>
                <td>{{ member.firstName }}</td>
            </tr>
            <tr>
                <th>Last_name</th>
                <td>{{ member.lastName }}</td>
            </tr>
            <tr>
                <th>Username</th>
                <td>{{ member.username }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ member.email }}</td>
            </tr>
            <tr>
                <th>Address1</th>
                <td>{{ member.address1 }}</td>
            </tr>
            <tr>
                <th>Address2</th>
                <td>{{ member.address2 }}</td>
            </tr>
            <tr>
                <th>Zipcode</th>
                <td>{{ member.zipcode }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ member.city }}</td>
            </tr>
            <tr>
                <th>Phone</th>
                <td>{{ member.phone }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('member_index') }}">back to list</a>

    <a href="{{ path('member_edit', {'id': member.id}) }}">edit</a>

    {{ include('member/_delete_form.html.twig') }}
{% endblock %}
