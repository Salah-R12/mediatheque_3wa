{% extends 'base.html.twig' %}

{% block title %}Hello DashboardController!{% endblock %}

{% block stylesheets %}

{% endblock }

{% block body %}
<h2></h2>
<div>
    <h2>Nouveautes : Musiques</h2>
    <ul class="list-group">
        {% for el in last_musics %}
            <li class="list-group-item"><a href='music/{{el.ID}}'>{{el.titles}}</a></li>
        {% endfor %}
    </ul>
</div>

<div>
    <h2>Nouveautes : Films</h2>
    <ul class="list-group">
        {% for el in last_films %}
            <li class="list-group-item"><a href='film/{{el.id}}'>{{el.original_title}}</a></li>
        {% endfor %}
    </ul>
</div>

<div>
    <h2>Nouveautes : Livres</h2>
    <ul class="list-group">
        {% for el in last_books %}
            <li class="list-group-item"><a href='book/{{el.id}}'>{{el.name}}</a></li>
        {% endfor %}
    </ul>
</div>
{% endblock %}


